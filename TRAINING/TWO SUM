class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        dict = {}
        for i, n in enumerate(nums):
            result = target - n
            if result in dict:
                return [dict[result], i]
            dict[n] = i
        return [i,n]
